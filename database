CREATE DATABASE symfony_api;
use symfony_api;
CREATE TABLE `users` (
ID int NOT NULL AUTO_INCREMENT PRIMARY KEY,
FirstName varchar(255),
LastName varchar(255),
Age int
);

INSERT INTO `users` (FirstName, LastName, Age)
VALUES
('John', 'Snow', 16),
('Adolf', 'Winter', 69),
('Bin', 'Purple', 45),
('Carl', 'Red', 13),
('Lucy', 'Blue', 4);
